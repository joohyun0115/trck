foreach %a, %b in @params
    start ->
        receive
            type=%b -> wait
            a=%a -> quit
            type in #w -> repeat
            * -> quit

    wait ->
        receive
            type='t1' -> yield a to #x, candidate
            * -> repeat

    candidate ->
        receive
            type='t2', timestamp >= %ts -> yield $y, yield a, type to #z, skip_dup
            * -> repeat

    skip_dup ->
        receive
           * -> repeat
        after 10m -> candidate
